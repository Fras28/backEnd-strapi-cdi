    },
    "detail": {
      "type": "text"
    },
    "price": {
      "type": "biginteger",
      "required": true
    },
    "quantity": {
      "type": "biginteger"
    },
    "status": {
      "type": "boolean",
      "required": true
    },
    "picture": {
      "type": "media",
      "multiple": false,
